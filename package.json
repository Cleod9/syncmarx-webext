{
  "name": "syncmarx",
  "version": "0.9.0",
  "description": "Web extension built for cross-browser bookmark syncing to the cloud.",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:Cleod9/syncmarx-webext.git"
  },
  "scripts": {
    "dev": "set NODE_ENV=dev && webpack",
    "dev-watch": "set NODE_ENV=dev && webpack --watch",
    "dev-web-ext": "set NODE_ENV=dev && webpack && cd app && web-ext run",
    "prod": "set NODE_ENV=prod && webpack",
    "prod-watch": "set NODE_ENV=prod && webpack && cd app && web-ext run",
    "prod-web-ext": "set NODE_ENV=prod && webpack && cd app && web-ext run",
    "dev-firefox": "set SYNCMARX_MANIFEST=firefox && npm run dev",
    "dev-watch-firefox": "set SYNCMARX_MANIFEST=firefox && npm run dev-watch",
    "dev-web-ext-firefox": "set SYNCMARX_MANIFEST=firefox && npm run dev-web-ext",
    "prod-firefox": "set SYNCMARX_MANIFEST=firefox && npm run prod",
    "prod-watch-firefox": "set SYNCMARX_MANIFEST=firefox && npm run prod-watch",
    "prod-web-ext-firefox": "set SYNCMARX_MANIFEST=firefox && npm run prod-web-ext"
  },
  "author": "",
  "browserslist": [
    "last 40 versions"
  ],
  "dependencies": {
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "@babel/preset-react": "7.18.6",
    "autoprefixer": "10.4.9",
    "babel-loader": "8.2.5",
    "box-javascript-sdk": "https://github.com/allenmichael/box-javascript-sdk.git#ccdb42df3a35be86fc10c6f223e2a8757dcd31bb",
    "bootstrap": "4.6.2",
    "classnames": "2.3.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.1.0",
    "cryptr": "3.0.0",
    "css-loader": "6.7.1",
    "detect-browser": "5.3.0",
    "dropbox": "10.32.0",
    "es6-promise": "4.2.8",
    "es6-symbol": "3.1.3",
    "lodash": "4.17.21",
    "lz-string": "1.4.4",
    "moment": "2.29.4",
    "node-polyfill-webpack-plugin": "2.0.1",
    "node-sass": "9.0.0",
    "normalize.css": "8.0.1",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "process": "0.11.10",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading": "2.0.3",
    "sass-loader": "14.2.1",
    "style-loader": "3.3.1",
    "webextension-polyfill": "0.12.0",
    "web-ext": "7.11.0",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0"
  }
}
